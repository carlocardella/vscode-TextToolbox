    "keybindings": [
      {
        "command": "vscode-texttoolbox.TabOut",
        "key": "tab",
        "when": "editorTextFocus && !suggestWidgetVisible && !inlineSuggestionVisible && !inSnippetMode && !editorHasMultipleSelections && !inlineEditIsVisible && !tabShouldJumpToInlineEdit"
      },
      {
        "command": "vscode-texttoolbox.SelectTextBetweenBrackets",
        "key": "ctrl+shift+alt+b",
        "mac": "ctrl+shift+cmd+b",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-texttoolbox.SelectTextBetweenQuotes",
        "key": "ctrl+shift+alt+q",
        "mac": "ctrl+shift+cmd+q",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-texttoolbox.RemoveQuotes",
        "key": "ctrl+shift+alt+'",
        "mac": "ctrl+shift+cmd+'",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-texttoolbox.RemoveBrackets",
        "key": "ctrl+shift+alt+;",
        "mac": "ctrl+shift+cmd+;",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-texttoolbox.CycleQuotes",
        "key": "ctrl+shift+alt+,",
        "mac": "ctrl+shift+cmd+,",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-texttoolbox.CycleBrackets",
        "key": "ctrl+shift+alt+/",
        "mac": "ctrl+shift+cmd+/",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-texttoolbox.OpenPathOrUrlUnderCursor",
        "key": "alt+o",
        "mac": "cmd+o",
        "when": "editorTextFocus"
      }
    ]
