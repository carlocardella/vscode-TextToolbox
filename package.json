        "configuration": [
            {
                "title": "Text Toolbox",
                "properties": {
                    "TextToolbox.statusBarAlignment": {
                        "type": "string",
                        "enum": [
                            "Left",
                            "Right"
                        ],
                        "default": "Right",
                        "scope": "window",
                        "description": "Text Toolbox word and selected lines count alignment in the StatusBar"
                    },
                    "TextToolbox.statusBarPriority": {
                        "type": "number",
                        "default": 100,
                        "scope": "window",
                        "description": "Text Toolbox word and selected lines count StatusBar priority"
                    },
                    "TextToolbox.enableStatusBarWordLineCount": {
                        "type": "boolean",
                        "default": true,
                        "scope": "window",
                        "description": "Enable or disable the StatusBar Text Toolbox word and selected lines count"
                    },
                    "TextToolbox.ignoreWhitespaceInLineFilters": {
                        "type": "boolean",
                        "default": true,
                        "scope": "window",
                        "description": "Ignore leading and trailing whitespaces when filtering lines: this can impact how duplicate lines are removed"
                    },
                    "TextToolbox.filtersUseRegularExpressions": {
                        "type": "boolean",
                        "default": true,
                        "scope": "window",
                        "description": "Filter use Regular Expression rather than simple text match"
                    },
                    "TextToolbox.decorateControlCharacters": {
                        "type": "object",
                        "description": "Control characters decoration appearance",
                        "default": {
                            "borderWidth": "1px",
                            "borderStyle": "solid",
                            "overviewRulerColor": "red",
                            "borderColor": "red"
                        }
                    },
                    "TextToolbox.replaceControlCharactersWith": {
                        "type": "string",
                        "default": "",
                        "scope": "window",
                        "description": "Replace control characters (Remove Control Characters) in the active document with this string"
                    },
                    "TextToolbox.alignTextDefaultSeparator": {
                        "type": "string",
                        "default": ",",
                        "scope": "window",
                        "description": "Align text default separator (Align Text)"
                    },
                    "TextToolbox.decorationDefaults": {
